{
  "name": "supersmashtexty",
  "version": "0.1.47",
  "description": "ASCII-style browser action roguelike inspired by Smash TV and Vampire Survivors",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "vite --port 3010",
    "build": "vite build",
    "preview": "vite preview",
    "git:status": "git status -sb",
