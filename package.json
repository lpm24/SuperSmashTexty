    "git:review": "git diff --cached",
    "git:recent": "git log --oneline --graph --decorate -10"
  },
  "keywords": [
    "game",
    "roguelike",
    "ascii",
    "kaplay",
    "browser-game"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "gh-pages": "^6.3.0",
    "vite": "^7.2.2"
  },
  "dependencies": {
    "kaplay": "^3001.0.19",
    "peer": "^1.0.2"
  }
}
